<%= render 'shared/error_messages' %>
<%= form_for @bookreceipt do |f| -%>
  <p>
    Crate: <%= f.text_field :crate_id %>
  </p>
  <p>
    ISBN: <%= f.text_field :isbn %>
  </p>
  <p>
    Book No.: <%= f.text_field :book_no %>
  </p>
  <p>
    <%= f.submit "Save" %>
    <%= f.submit "Cancel" %>
  </p>
<% end %>
