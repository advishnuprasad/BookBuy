<div class='span-20 last'>
  <table class='no-zebra'>
    <tr>
      <th style='width:15%'></th>
      <th>AMS</th>
      <th>Justbooksclc.com</th>
      <th>Internet</th>
    </tr>
    <tr>
      <th>Title Id</th>
      <td><%= @enrichedtitle.title_id %></td>
      <td><%= @enrichedtitle.jbtitle.try(:id) %></td>        
      <td></td>        
    </tr>
    <tr>
      <th>ISBN</th>
      <td><%= @enrichedtitle.isbn %></td>
      <td><%= @enrichedtitle.jbtitle.try(:isbn_13) %></td>
      <td><%= @enrichedtitle.isbn %></td>
    </tr>
    <tr>
      <th>Title</th>
      <td><%= @enrichedtitle.title %></td>
      <td><%= @enrichedtitle.jbtitle.try(:title) %></td>
      <td><%= @enrichedtitle.web_title %></td>
    </tr>
    <tr>
      <th>Language</th>
      <td><%= @enrichedtitle.language %></td>
      <td><%= @enrichedtitle.jbtitle.try(:language) %></td>
      <td><%= @enrichedtitle.web_language %></td>
    </tr>
    <tr>
      <th>Author</th>
      <td><%= @enrichedtitle.author %></td>
      <td><%= @enrichedtitle.jbtitle.try(:author).try(:name) %></td>
      <td><%= @enrichedtitle.web_author %></td>
    </tr>
    <tr>
      <th>Category</th>
      <td><%= @enrichedtitle.jbcategory.try(:division) %> > <%= @enrichedtitle.jbcategory.try(:name) %></td>
      <td><%= @enrichedtitle.jbtitle.try(:category).try(:division) %> > <%= @enrichedtitle.jbtitle.try(:category).try(:name) %></td>
      <td><%= @enrichedtitle.web_category %></td>
    </tr>
    <tr>
      <th>Imprint</th>
      <td><%= @enrichedtitle.imprint.try(:name) %></td>
      <td></td>
      <td></td>
    </tr>
    <tr>
      <th>Publisher</th>
      <td><%= @enrichedtitle.imprint.try(:publisher).try(:name) || "?" %></td>
      <td><%= @enrichedtitle.jbtitle.try(:publisher).try(:name) || "?" %></td>
      <td><%= @enrichedtitle.publisher_name %></td>
    </tr>
    <tr>
      <th>List Price</th>
      <td><%= @enrichedtitle.currency %> <%= @enrichedtitle.listprice %></td>
      <td><%= @enrichedtitle.jbtitle.try(:mrp) %></td>
      <td><%= @enrichedtitle.web_listprice %></td>
    </tr>
    <tr>
      <th>Pub Year</th>
      <td><%= @enrichedtitle.pub_year %></td>
      <td><%= @enrichedtitle.jbtitle.try(:yearofpublication) %></td>
      <td><%= @enrichedtitle.pub_year %></td>
    </tr>
    <tr>
      <th>Pages</th>
      <td><%= @enrichedtitle.page_cnt %></td>
      <td><%= @enrichedtitle.jbtitle.try(:no_of_pages) %></td>
      <td><%= @enrichedtitle.page_cnt %></td>
    </tr>
    <tr>
      <th>Format</th>
      <td><%= @enrichedtitle.format %></td>
      <td><%= @enrichedtitle.jbtitle.try(:format) %></td>
      <td><%= @enrichedtitle.format %></td>
    </tr>
    <tr>
      <th>Image?</th>
      <td><%= !@enrichedtitle.cover.nil? %></td>
      <td><%= @enrichedtitle.jbtitle.try(:flag_isbn_image) %></td>
      <td></td>
    </tr>
    <tr>
      <th>Last Changed</th>
      <td><%= @enrichedtitle.updated_at %></td>
      <td><%= @enrichedtitle.jbtitle.try(:insertdate) %></td>
      <td></td>
    </tr>
    <tr>
      <th>User</th>
      <td></td>
      <td><%= @enrichedtitle.jbtitle.try(:userid) %></td>
      <td><%= @enrichedtitle.web_scanned %></td>
    </tr>
  </table>
</div>


<div class="span-3 last">
  Our Image: <br/>
  <%= image_tag @enrichedtitle.cover.url, :size => "75X150" %>  
</div>
