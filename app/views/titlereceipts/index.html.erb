<div>
  <div>
    <% if @titlereceipts %>
      <%= will_paginate :prev_label => '<< Previous', :next_label => 'Next >>', :class => 'pagination'%>
        <% @titlereceipts.each do |titlereceipt| %>
          <div class="ui-widget-content ui-corner-all list-item-row">
            <table style="width: 100%;">
              <tr>
                <td style="width: 10%;"><%= link_to titlereceipt.id, edit_titlereceipt_path(titlereceipt.id) %></td>
                <td style="width: 20%;"><%= titlereceipt.po_no %></td>
                <td style="width: 10%;"><%= titlereceipt.invoice_no %></td>
                <td style="width: 10%;"><%= titlereceipt.isbn %></td>
                <td style="width: 10%;"><%= titlereceipt.box_no %></td>
                <td style="width: 10%;"><%= titlereceipt.created_at.strftime('%d-%b-%Y') %></td>
              </tr>
            </table>
          </div>
        <% end %>
      <%= will_paginate %>
    <% end %>
  </div>
</div>
